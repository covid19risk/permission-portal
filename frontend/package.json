{
  "name": "covid-watch",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "init": "cpy ../config/firebase* client/src/config/",
    "test": "jest",
    "test_ci": "npm run init && NODE_ENV=test npm run test",
    "start": "webpack-dev-server --open --hot --config webpack.dev.js",
    "start:local": "webpack-dev-server --open --hot --config webpack.dev.js --define process.env.NODE_ENV='\"local\"'",
    "start:dev": "webpack-dev-server --open --hot --config webpack.dev.js --define process.env.NODE_ENV='\"development\"'",
    "start:test": "webpack-dev-server --open --hot --config webpack.dev.js --define process.env.NODE_ENV='\"test\"'",
    "build": "webpack --config webpack.prod.js",
    "build:staging": "webpack --define process.env.NODE_ENV='\"staging\"' --config webpack.prod.js",
    "debug": "node --nolazy --inspect-brk=9230 ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --open --hot",
    "debug:dev": "NODE_ENV=dev npm run debug",
    "debug:local": "NODE_ENV=local npm run debug",
    "build_win": "set NODE_ENV=production & webpack --config webpack.prod.js",
    "dev_win": "set NODE_ENV=development & webpack-dev-server --open --hot --config webpack.dev.js",
    "lint": "eslint --fix .",
    "format": "prettier --write \"**/*.+(js|jsx|json|yml|yaml|css|md|vue)\"",
    "lint-and-format": "npm run lint && npm run format"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-and-format"
    }
  },
